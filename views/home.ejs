<%- include('partials/header') %>

    <h1>Home</h1>
    <p>
        <%= homeStartingContent %>
    </p>

    <% posts.map(el=> {%>
        <h1>
            <%= el.title %>
        </h1>

        <p>
            <%= el.text.substring(0,100) + "..." %>
                <a href=<%="/posts/" +_.kebabCase(el.title) %>
                    >
                    Read More
                </a>
        </p>

        <% }) %>


            <%- include('partials/footer') %>